{
  "name": "expo-live-updates",
  "version": "0.0.0",
  "description": "Library based on expo modules for Android Live Updates",
  "keywords": [
    "Android",
    "Live Updates"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "lint:libOnly": "expo-module eslint --ignore-pattern 'example/*'",
    "test": "expo-module test",
    "prepare": "expo-module prepare && rm .eslintrc.js",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "open:ios": "xed example/ios",
    "open:android": "open -a \"Android Studio\" example/android",
    "format:check": "prettier --check .",
    "format:fix": "prettier --check -w .",
    "postinstall": "cd example && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/software-mansion-labs/expo-live-updates.git"
  },
  "bugs": {
    "url": "https://github.com/software-mansion-labs/expo-live-updates/issues"
  },
  "license": "MIT",
  "author": "Kamil Owczarz",
  "homepage": "https://github.com/software-mansion-labs/expo-live-updates#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/react": "~19.0.0",
    "expo-module-scripts": "^4.1.10",
    "expo": "~53.0.0",
    "react-native": "0.79.1",
    "eslint": "^9.35.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  }
}
